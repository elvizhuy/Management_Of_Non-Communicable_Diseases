<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="411.0" prefWidth="1016.0" stylesheets="@Style/LoginCss.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.devteam.management_of_noncommunicable_diseases.Controller.LoginController">
   <children>
      <TextField fx:id="userField" layoutX="657.0" layoutY="204.0" prefHeight="31.0" prefWidth="315.0" promptText="Username">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passField" layoutX="658.0" layoutY="254.0" prefHeight="31.0" prefWidth="313.0" promptText="Password">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnLogin" layoutX="658.0" layoutY="323.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="313.0" text="Button" />
      <ImageView fitHeight="440.0" fitWidth="601.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Image/LoginBack4.png" />
         </image>
      </ImageView>
      <ImageView fx:id="btnCloseLogin" accessibleRole="BUTTON" fitHeight="14.0" fitWidth="15.0" layoutX="573.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Image/close.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="99.0" fitWidth="100.0" layoutX="765.0" layoutY="40.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Image/IconBack3.png" />
         </image>
      </ImageView>
      <Hyperlink layoutX="862.0" layoutY="291.0" text="Forgot Password ? " />
      <Text fill="#00000083" layoutX="737.0" layoutY="169.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login to continue">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <CheckBox layoutX="657.0" layoutY="294.0" mnemonicParsing="false" text="Keep me logged in" />
   </children>
</AnchorPane>
